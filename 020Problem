#include<iostream>
using namespace std;

int A[100], B[100];
int main(){
	int i, n;
	
	cin>>n;
	
	for(i=0; i<n; i++){
		cin>>A[i];
	}
	
	for(i=0; i<n; i++){
		cin>>B[i];
	}
	
	for(i=0; i<n; i++){
		if(A[i]==B[i]){
			cout<<"D"<<"\n";
		}else if(A[i]==1 && B[i]==3){
			cout<<"A"<<"\n";
		}else if(A[i]==2 && B[i]==1){
			cout<<"A"<<"\n";	
		}else if(A[i]==3 && B[i]==2){
			cout<<"A"<<"\n";
		}
		else{
			cout<<"B"<<"\n";
		}
	}

	return 0;
} 
